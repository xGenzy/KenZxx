{
  "name": "whatsapp-bot",
  "version": "1.0.0",
  "description": "WhatsApp Bot with auto-deploy",
  "main": "wabot.js",
  "scripts": {
    "start": "node wabot.js",
    "dev": "nodemon wabot.js",
    "install-deps": "npm install",
    "pm2": "pm2 start ecosystem.config.js",
    "pm2:monit": "pm2 monit",
    "pm2:logs": "pm2 logs",
    "pm2:restart": "pm2 restart whatsapp-bot",
    "pm2:stop": "pm2 stop whatsapp-bot",
    "deploy": "bash scripts/deploy.sh"
  },
  "dependencies": {
    "@whiskeysockets/baileys": "^7.0.0-rc.9",
    "axios": "^1.13.2",
    "btch-downloader": "^6.0.22",
    "cheerio": "^1.1.2",
    "ffmpeg": "^0.0.4",
    "fluent-ffmpeg": "^2.1.3",
    "instagram-private-api": "^1.46.1",
    "moment-timezone": "^0.6.0",
    "qs": "^6.14.0",
    "pm2": "^5.3.0",
    "nodemon": "^3.0.1"
  },
  "keywords": ["whatsapp", "bot", "baileys"],
  "author": "Kenzo",
  "license": "MIT"
}
